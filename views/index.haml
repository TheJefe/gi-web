%h2
  = label
  |
  %a{href: link[:endpoint]}= link[:label]
%table{:border => '1', :cellpadding => '2'}
  %thead
    %tr
      %th Id
      %th Description
      %th Label Applied
      %th Assignee
  %tbody
  - list.each do |item|
    %tr
      %td= item['number']
      %td= item['title']
      %td= item['label_event_data']['created_at']
      %td
        - if item['assignee']
          %img{ src: item['assignee']['avatar_url'], height: 20, width: 20 }= item['assignee']['login']
