%h2= label
%table{:border => '1', :cellpadding => '2'}
  %thead
    %tr
      %th Id
      %th Description
      %th Label Applied
      %th Assignee
  %tbody
  - issues.each do |issue|
    %tr
      %td= issue['number']
      %td= issue['title']
      %td= issue['label_event_data']['created_at']
      %td
        - if issue['assignee']
          %img{ src: issue['assignee']['avatar_url'], height: 20, width: 20 }= issue['assignee']['login']
